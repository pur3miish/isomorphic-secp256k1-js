{
  "name": "universal-secp256k1-js",
  "version": "1.0.0-rc",
  "description": "An ultra lightweight zero dependency universal JavaScript (ECDSA) secp256k1 curve.",
  "engines": {
    "node": ">=15"
  },
  "scripts": {
    "prepare": "npm run prepare:jsdoc && npm run prepare:prettier",
    "prepare:jsdoc": "jsdoc-md",
    "prepare:prettier": "prettier --write readme.md",
    "test": "npm run test:eslint && npm run test:prettier && npm run test:api",
    "test:api": "coverage-node test/index.test.mjs",
    "test:eslint": "eslint .",
    "test:prettier": "prettier -c .",
    "prepublishOnly": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pur3miish/universal-secp256k1-js.git"
  },
  "bugs": {
    "url": "https://github.com/pur3miish/universal-secp256k1-js/issues"
  },
  "homepage": "https://github.com/pur3miish/universal-secp256k1-js#readme",
  "exports": {
    "./package.json": "./package.json",
    ".": {
     "require": "./index.js",
     "import": "./index.mjs"
    }
  },
  "keywords": [
    "ECDSA",
    "universal",
    "blockchain",
    "digital",
    "signature",
    "ecc",
    "elliptic",
    "curve",
    "crypto",
    "currencies",
    "secp256k1",
    "javascript",
    "zero",
    "no",
    "dependencies",
    "lightweight"
  ],
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "files": [
    "public",
    "private"
  ],
  "author": "pur3miish",
  "license": "MIT",
  "dependencies": {
    "universal-hmac-sha256-js": "^1.0.0",
    "universal-sha256-js": "^1.0.0"
  },
  "devDependencies": {
    "coverage-node": "^8.0.0",
    "eslint": "^8.25.0",
    "eslint-config-env": "^23.0.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^37.9.7",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jsdoc-md": "^11.0.2",
    "prettier": "^2.7.1",
    "test-director": "^10.0.0"
  }
}
